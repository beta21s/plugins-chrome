{
    "name": "Lịch học",
    "description": "Công cụ chọn nhanh lịch học v1",
    "version": "1.0",
    "browser_action": {},
    "content_scripts": [
        {
          "matches": [ 
              "http://*/*",
              "https://*/*"
            ],
          "js": [
              "jquery.min.js",
              "background.js"
            ],
            "run_at": "document_end"
        }
      ],
    "manifest_version": 2,
    "web_accessible_resources":  [
      "jquery.min.js",
      "background.js"
    ],
    "background": {
      "scripts":  [
        "background.js"
      ],
      "persistent": true
  }
}